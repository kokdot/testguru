<% flash.each do |key, message| %>
  <% if key == 'notice' %>
    <%= content_tag :p, message, class: "alert alert-primary" %>
  <% elsif key == 'alert'%>
    <%= content_tag :p, message, class: "alert alert-warning" %>
  <% else %>
    <%= content_tag :p, message, class: "alert alert-info" %>
  <% end %>
<% end %>
